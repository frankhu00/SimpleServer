{
    "name": "simpleserver",
    "version": "1.0.0",
    "description": "SimpleServer",
    "main": "./src/server.js",
    "scripts": {
        "start": "cross-env-shell NODE_ENV=development \"npm run build && npm run start-dev-server\"",
        "start-prod": "cross-env-shell NODE_ENV=production \"npm run bundle && node ./lib/server.js\"",
        "start-dev-server": "babel-node -- ./src/server.js",
        "start-server": "cross-env-shell NODE_ENV=production \"node ./lib/server.js\"",
        "build": "webpack --mode development --config webpack.dev.config.js",
        "build-prod": "webpack --mode production --config webpack.prod.config.js",
        "transpile": "npx babel src -d lib --copy-files --delete-dir-on-start",
        "bundle": "npm run build-prod && npm run transpile",
        "test": "mocha --exit --require @babel/register --require @babel/polyfill ./tests/**/*.test.js",
        "test-file": "mocha --exit --require @babel/register --require @babel/polyfill "
    },
    "author": "Frank Hu",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.19.0",
        "chalk": "^2.4.2",
        "express": "^4.17.1",
        "moment": "^2.24.0",
        "mysql2": "^1.6.5",
        "normalize.css": "^8.0.1",
        "polished": "^3.4.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "styled-components": "^4.3.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.6",
        "@babel/node": "^7.5.5",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-proposal-decorators": "^7.1.6",
        "@babel/plugin-proposal-do-expressions": "^7.0.0",
        "@babel/plugin-proposal-export-default-from": "^7.0.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
        "@babel/plugin-proposal-function-bind": "^7.0.0",
        "@babel/plugin-proposal-function-sent": "^7.1.0",
        "@babel/plugin-proposal-json-strings": "^7.0.0",
        "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
        "@babel/plugin-proposal-numeric-separator": "^7.0.0",
        "@babel/plugin-proposal-optional-chaining": "^7.0.0",
        "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
        "@babel/plugin-proposal-throw-expressions": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-syntax-import-meta": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.5.5",
        "@babel/polyfill": "^7.4.4",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "@babel/register": "^7.5.5",
        "@babel/runtime": "^7.5.5",
        "babel-loader": "^8.0.6",
        "chai": "^4.2.0",
        "compression-webpack-plugin": "^3.1.0",
        "cross-env": "^5.2.0",
        "css-loader": "^3.1.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^4.1.0",
        "html-webpack-plugin": "^5.5.0",
        "mocha": "^6.2.0",
        "prettier": "^1.18.2",
        "progress-bar-webpack-plugin": "^1.12.1",
        "sinon": "^7.3.2",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.38.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-middleware": "^3.7.0",
        "webpack-dev-server": "^3.7.2",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-merge": "^4.2.1"
    }
}
